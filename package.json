{
  "name": "rush",
  "scripts": {
    "start": "node ./packages.js --start & npm run build & sh ./dev.sh",
    "build": "rush build:watch --to-except react-demo",
    "auth": "sh ./auth.sh",
    "packages": "node ./packages.js",
    "change": "rush change --bulk --message 'update' --bump-type patch",
    "publish": "sh ./auth.sh && rush publish -a -p"
  }
}